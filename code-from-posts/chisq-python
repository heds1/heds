    "# post-hoc tests\n",
    "\n",
    "combns = [\n",
    "    [4, 6],\n",
    "    [4, 8],\n",
    "    [6, 8]\n",
    "]\n",
    "\n",
    "for i in combns:\n",
    "    this_subset = mpg[mpg['cylinders'].isin(i)]\n",
    "    contingency_tbl = pd.crosstab(this_subset['mpg_group'], this_subset['cylinders'])\n",
    "    print('Comparing ' + i)\n",
    "    print(scipy.stats.chi2_contingency(contingency_tbl))"
   ]
